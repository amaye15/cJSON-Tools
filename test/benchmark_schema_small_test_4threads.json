{
	"$schema":	"http://json-schema.org/draft-07/schema#",
	"type":	"object",
	"properties":	{
		"id":	{
			"type":	"string"
		},
		"name":	{
			"type":	"string"
		},
		"email":	{
			"type":	"string"
		},
		"age":	{
			"type":	"integer"
		},
		"active":	{
			"type":	"boolean"
		},
		"address":	{
			"type":	"object",
			"properties":	{
				"street":	{
					"type":	"string"
				},
				"city":	{
					"type":	"string"
				},
				"state":	{
					"type":	"string"
				},
				"zipcode":	{
					"type":	"string"
				}
			},
			"required":	["street", "city", "state", "zipcode"]
		},
		"tags":	{
			"type":	"array",
			"items":	{
				"type":	"string"
			}
		},
		"preferences":	{
			"type":	"object",
			"properties":	{
				"notifications":	{
					"type":	"boolean"
				},
				"newsletter":	{
					"type":	"boolean"
				},
				"theme":	{
					"type":	"integer"
				},
				"display":	{
					"type":	"object",
					"properties":	{
						"fontSize":	{
							"type":	"integer"
						},
						"darkMode":	{
							"type":	"boolean"
						}
					},
					"required":	["fontSize", "darkMode"]
				}
			},
			"required":	["notifications", "newsletter", "theme", "display"]
		},
		"history":	{
			"type":	"array",
			"items":	{
				"type":	"object",
				"properties":	{
					"action":	{
						"type":	"string"
					},
					"timestamp":	{
						"type":	"integer"
					},
					"details":	{
						"type":	"object",
						"properties":	{
							"ip":	{
								"type":	"string"
							},
							"device":	{
								"type":	"string"
							}
						},
						"required":	["ip", "device"]
					}
				},
				"required":	["action", "timestamp", "details"]
			}
		}
	},
	"required":	["id", "name", "email", "age", "active", "address", "tags", "preferences", "history"]
}

{
	"$schema":	"http://json-schema.org/draft-07/schema#",
	"type":	"object",
	"properties":	{
		"address":	{
			"type":	"object",
			"properties":	{
				"zipcode":	{
					"type":	"string"
				},
				"city":	{
					"type":	"string"
				},
				"street":	{
					"type":	"string"
				}
			},
			"required":	["zipcode", "city", "street"]
		},
		"tags":	{
			"type":	"array",
			"items":	{
				"type":	"string"
			}
		},
		"active":	{
			"type":	"boolean"
		},
		"age":	{
			"type":	"integer"
		},
		"email":	{
			"type":	"string"
		},
		"name":	{
			"type":	"string"
		},
		"id":	{
			"type":	"integer"
		}
	},
	"required":	["address", "tags", "active", "age", "email", "name", "id"]
}

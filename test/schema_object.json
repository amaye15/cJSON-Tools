{
	"$schema":	"http://json-schema.org/draft-07/schema#",
	"type":	"object",
	"properties":	{
		"salary":	{
			"type":	"number"
		},
		"active":	{
			"type":	"boolean"
		},
		"education":	{
			"type":	"array",
			"items":	{
				"type":	"object",
				"properties":	{
					"year":	{
						"type":	"integer"
					},
					"field":	{
						"type":	"string"
					},
					"degree":	{
						"type":	"string"
					}
				},
				"required":	["year", "field", "degree"]
			}
		},
		"skills":	{
			"type":	"array",
			"items":	{
				"type":	"string"
			}
		},
		"contact":	{
			"type":	"object",
			"properties":	{
				"phone":	{
					"type":	"object",
					"properties":	{
						"mobile":	{
							"type":	"string"
						},
						"home":	{
							"type":	"string"
						}
					},
					"required":	["mobile", "home"]
				},
				"email":	{
					"type":	"string"
				}
			},
			"required":	["phone", "email"]
		},
		"address":	{
			"type":	"object",
			"properties":	{
				"zipcode":	{
					"type":	"string"
				},
				"city":	{
					"type":	"string"
				},
				"street":	{
					"type":	"string"
				}
			},
			"required":	["zipcode", "city", "street"]
		},
		"age":	{
			"type":	"integer"
		},
		"name":	{
			"type":	"string"
		}
	},
	"required":	["salary", "active", "education", "skills", "contact", "address", "age", "name"]
}

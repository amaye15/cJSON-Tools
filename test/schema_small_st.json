{
	"$schema":	"http://json-schema.org/draft-07/schema#",
	"type":	"object",
	"properties":	{
		"active":	{
			"type":	"boolean"
		},
		"phones":	{
			"type":	"array",
			"items":	{
				"type":	"object",
				"properties":	{
					"number":	{
						"type":	"string"
					},
					"type":	{
						"type":	"string"
					}
				},
				"required":	["number", "type"]
			}
		},
		"address":	{
			"type":	"object",
			"properties":	{
				"zip":	{
					"type":	"string"
				},
				"state":	{
					"type":	"string"
				},
				"city":	{
					"type":	"string"
				},
				"street":	{
					"type":	"string"
				}
			},
			"required":	["zip", "state", "city", "street"]
		},
		"age":	{
			"type":	"integer"
		},
		"name":	{
			"type":	"string"
		}
	},
	"required":	["active", "phones", "address", "age", "name"]
}
